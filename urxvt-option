#!/bin/sh

# How to use this file?
#
#  Start Emacs or Emacsclinet like this:
#
#    #!/bin/sh
#    urxvtopt=path/to/this-repository/urxvt-option
#    eval "urxvt `${urxvtopt}` -e emacsclient -t -a ''"

# Emacs' configuration:
# (add-hook 'tty-setup-hook
#           '(lambda ()
#              (when (getenv "COLORTERM" (selected-frame))
#                ;; Those font and size arguments are optional
#                ;; if you specify those both args, you can change font size
#                ;; by text-scale-increase or text-scale-decrease.
#                (urxvt-keybinder-setup "xft:DejaVu Sans Mono" 12))))

